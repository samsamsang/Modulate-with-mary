import numpy as np
import pandas as pd 
import matplotlib.pyplot as put
signal=pd.read_csv('data_01.csv',header:3,nrows=4000)
def show_graph(signal):
 T=signal('Time(s)')
 V=signal('Voltage(V)')
 plt.plot(T,V,label:'signal')
 plt.title('raw_signal')
 plt.xlabel('time(s)')
 plt.ylabel('Voltage(V)')
 plt.legend()
 plt.show()
def transform_fourier(signal):
 V=signal('Voltage(V)')
 freq=np.fft.fftfreq(2000,0.0004)
 mag=np.abs(np.fft.fft(V))
 plt.show
 return(np.fft.fft(V))


